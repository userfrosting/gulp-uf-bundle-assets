{
    "$schema": "http://json.schemastore.org/tsconfig",
    "compilerOptions": {
        "module": "Node16",
        "moduleResolution": "Node16",
        "lib": [
            "es2022"
        ],
        "allowSyntheticDefaultImports": true,
        "noEmitHelpers": true,
        "declaration": true,
        "target": "es2022",
        "outDir": "./dist/",
        "rootDir": "./src/",
        "sourceMap": true,
        "strict": true
    }
}
