    "vinyl": "^2.2.0"
  },
  "devDependencies": {
    "@types/just-extend": "^1.1.0",
    "@types/node": "^10.12.9",
    "@types/vinyl": "^2.0.2",
    "ava": "^1.0.1",
    "changelog-updater": "^1.1.0",
    "typescript": "^3.1.6"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "engineStrict": true,
  "ava": {
    "files": [
      "dist/**/*.test.js"
    ],
    "require": [
      "esm"
